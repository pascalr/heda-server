{
  "name": "heda-server",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node ./src/server.js",
    "images:backup": "rsync -v --recursive root@heda-server.internal:/mnt/heda_volume/images/ volume/images/",
    "db:open": "sqlitebrowser var/db/dev.db",
    "db:backup": "scp root@heda-server.internal:/mnt/heda_volume/prod.db var/db/dev.db",
    "dev": "nodemon -e js,ejs -w views --trace-uncaught -r dotenv/config ./src/server.js",
    "build:all": "npm run build:css && npm run build:js",
    "build": "esbuild src/react/app.jsx --bundle --sourcemap --outfile=public/build/app.js",
    "build:show_user": "esbuild src/react/show_user.jsx --bundle --sourcemap --outfile=public/build/show_user.js",
    "build:show_recipe": "esbuild src/react/show_recipe.jsx --bundle --sourcemap --outfile=public/build/show_recipe.js",
    "build:user_editor": "esbuild src/react/user_editor.jsx --bundle --sourcemap --outfile=public/build/user_editor.js",
    "build:home": "esbuild src/react/home.jsx --bundle --sourcemap --outfile=public/build/home.js",
    "build:prod": "esbuild src/react/app.jsx --bundle --minify --outfile=public/build/app.js",
    "build:css": "sass src/scss/prose_mirror.scss:public/build/prose_mirror.css src/scss/imports.scss:public/build/imports.css"
  },
  "dependencies": {
    "@aeaton/prosemirror-commands": "^2.0.2",
    "@aeaton/react-prosemirror": "^2.0.3",
    "@popperjs/core": "^2.11.6",
    "@tiptap/core": "^2.0.0-beta.196",
    "@tiptap/extension-bubble-menu": "^2.0.0-beta.196",
    "@tiptap/extension-link": "^2.0.0-beta.196",
    "@tiptap/extension-subscript": "^2.0.0-beta.196",
    "@tiptap/extension-superscript": "^2.0.0-beta.196",
    "@tiptap/react": "^2.0.0-beta.196",
    "@tiptap/starter-kit": "^2.0.0-beta.196",
    "@tiptap/suggestion": "^2.0.0-beta.196",
    "backblaze": "^2.3.0",
    "better-sqlite3": "^7.6.2",
    "bootstrap": "^5.2.1",
    "connect-ensure-login": "^0.1.1",
    "connect-sqlite3": "^0.9.13",
    "cookie-parser": "~1.4.4",
    "csurf": "^1.11.0",
    "debug": "~2.6.9",
    "dotenv": "^16.0.2",
    "ejs": "~3.1.8",
    "esbuild": "^0.15.8",
    "express": "~4.16.1",
    "express-fileupload": "^1.4.0",
    "express-session": "^1.17.2",
    "http-errors": "~1.6.3",
    "jquery": "^3.6.1",
    "js-autocomplete": "^1.0.4",
    "mkdirp": "^1.0.4",
    "morgan": "~1.9.1",
    "passport": "^0.6.0",
    "passport-local": "^1.0.0",
    "pluralize": "^8.0.0",
    "prosemirror-example-setup": "^1.2.1",
    "prosemirror-markdown": "^1.10.0",
    "prosemirror-schema-basic": "^1.2.0",
    "prosemirror-state": "^1.4.1",
    "prosemirror-view": "^1.28.3",
    "punycode": "^2.1.1",
    "react": "^17.0.1",
    "react-autosuggest": "^10.1.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-bootstrap": "^2.5.0",
    "react-dom": "^17.0.1",
    "sass": "^1.54.9",
    "toastr": "^2.1.4",
    "watch": "~0.13.0"
  },
  "devDependencies": {
    "connect-livereload": "^0.6.1",
    "livereload": "^0.9.3",
    "nodemon": "^2.0.20"
  }
}
